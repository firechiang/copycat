<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ui_component_senior.UI01YoukuMenu">

    <RelativeLayout
        android:id="@+id/ui_comsenior01_youku_menu_level3"
        android:layout_width="280dp"
        android:layout_height="140dp"
        android:layout_alignParentBottom="true"
        android:layout_centerHorizontal="true"
        android:background="@mipmap/youku_menu_level3">

        <ImageView
            android:id="@+id/ui_comsenior01_youku_menu_channel1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentBottom="true"
            android:layout_marginBottom="8dp"
            android:layout_marginLeft="8dp"
            android:src="@mipmap/youku_menu_channel1" />

        <ImageView
            android:id="@+id/ui_comsenior01_youku_menu_channel2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_above="@id/ui_comsenior01_youku_menu_channel1"
            android:layout_marginBottom="8dp"
            android:layout_marginLeft="33dp"
            android:src="@mipmap/youku_menu_channel2" />


        <ImageView
            android:id="@+id/ui_comsenior01_youku_menu_channel3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_above="@id/ui_comsenior01_youku_menu_channel2"
            android:layout_marginBottom="8dp"
            android:layout_marginLeft="63dp"
            android:src="@mipmap/youku_menu_channel3" />

        <ImageView
            android:id="@+id/ui_comsenior01_youku_menu_channel4"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerHorizontal="true"
            android:layout_marginTop="8dp"
            android:src="@mipmap/youku_menu_channel4" />

        <ImageView
            android:id="@+id/ui_comsenior01_youku_menu_channel7"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentBottom="true"
            android:layout_alignParentRight="true"
            android:layout_marginBottom="8dp"
            android:layout_marginRight="8dp"
            android:src="@mipmap/youku_menu_channel7" />

        <ImageView
            android:layout_alignParentRight="true"
            android:id="@+id/ui_comsenior01_youku_menu_channel6"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_above="@id/ui_comsenior01_youku_menu_channel7"
            android:layout_marginBottom="8dp"
            android:layout_marginRight="33dp"
            android:src="@mipmap/youku_menu_channel6" />

        <ImageView
            android:layout_alignParentRight="true"
            android:id="@+id/ui_comsenior01_youku_menu_channel5"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_above="@mipmap/youku_menu_channel6"
            android:layout_marginBottom="8dp"
            android:layout_marginRight="63dp"
            android:src="@mipmap/youku_menu_channel5" />


    </RelativeLayout>


    <RelativeLayout
        android:id="@+id/ui_comsenior01_youku_menu_level2"
        android:layout_width="180dp"
        android:layout_height="90dp"
        android:layout_alignParentBottom="true"
        android:layout_centerHorizontal="true"
        android:background="@mipmap/youku_menu_level2">

        <ImageView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentBottom="true"
            android:layout_marginBottom="8dp"
            android:layout_marginLeft="8dp"
            android:src="@mipmap/youku_menu_icon_search" />


        <ImageView
            android:id="@+id/ui_comsenior01_youku_menu_icon_menu"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerHorizontal="true"
            android:layout_marginTop="8dp"
            android:src="@mipmap/youku_menu_icon_menu" />

        <ImageView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentBottom="true"
            android:layout_alignParentRight="true"
            android:layout_marginBottom="8dp"
            android:layout_marginRight="8dp"
            android:src="@mipmap/youku_menu_icon_myyouku" />


    </RelativeLayout>


    <RelativeLayout
        android:id="@+id/ui_comsenior01_youku_menu_level1"
        android:layout_width="100dp"
        android:layout_height="50dp"
        android:layout_alignParentBottom="true"
        android:layout_centerHorizontal="true"
        android:background="@mipmap/youku_menu_level1">


        <ImageView
            android:id="@+id/ui_comsenior01_youku_menu_icon_home"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerInParent="true"
            android:src="@mipmap/youku_menu_icon_home" />

    </RelativeLayout>


</RelativeLayout>
